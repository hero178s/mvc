@model IEnumerable<QLTA.Models.LoaiThucAn>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Loại thức ăn</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TenTL)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.TenTL)
        </td>
    </tr>
}

</table>
